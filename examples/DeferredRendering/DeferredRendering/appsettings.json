{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "DeferredRendering"
    }
  },
  "WindowSettings": {
    "ResolutionWidth": 1920,
    "ResolutionHeight": 1080,
    "ResolutionScale": 1.0,
    "WindowMode": "WindowedBorderless",
    "IsVsyncEnabled": true
  },
  "ContextSettings": {
    "TargetGLVersion": "4.6",
    "IsDebugContext": true
  }
}